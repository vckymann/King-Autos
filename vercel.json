{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "public/dist/**", "use": "@vercel/static" },
    { "src": "public/src/**", "use": "@vercel/static" },
    { "src": "public/scripts/**", "use": "@vercel/static" },
    { "src": "public/images/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
